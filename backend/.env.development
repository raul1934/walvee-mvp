# Local development environment variables (do NOT commit real secrets)
NODE_ENV=development
PORT=3000
API_VERSION=v1

# Database
DB_HOST=localhost
DB_PORT=3306
DB_NAME=walvee
DB_USER=root
DB_PASSWORD=your_mysql_password_here

# JWT
JWT_SECRET=your_super_secret_jwt_key_here_change_in_production
JWT_EXPIRES_IN=7d
JWT_REFRESH_SECRET=your_super_secret_refresh_key_here_change_in_production
JWT_REFRESH_EXPIRES_IN=30d

# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_CALLBACK_URL=http://localhost:3000/v1/auth/google/callback

# Frontend URL
FRONTEND_URL=http://localhost:5173

# Session
SESSION_SECRET=your_session_secret_here_change_in_production

# Uploads
UPLOAD_DIR=uploads
MAX_FILE_SIZE=5242880

# Optional services
GOOGLE_MAPS_API_KEY=

# Google Gemini API (for AI-powered features like trip recommendations)
# Use a real key in local development if you want to call Gemini
GEMINI_API_KEY=

# Gemini model configuration (override to control which models are used)
GEMINI_MODEL_DEFAULT=gemini-2.5-flash
GEMINI_MODEL_RECOMMENDATIONS=
GEMINI_MODEL_ORGANIZE=
GEMINI_MODEL_TRIP_MODIFICATION=gemini-2.0-flash-exp
GEMINI_MODEL_PLACE_REVIEW=gemini-2.0-flash-lite-001
GEMINI_MODEL_CHAT_DEFAULT=gemini-2.0-flash-lite-001

# Cloud Storage - leave blank for local dev
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_REGION=us-east-1
# AWS_S3_BUCKET=

# Production notes
# Set proper secrets and keys in production environment.